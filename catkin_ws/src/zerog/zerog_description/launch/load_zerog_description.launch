    <arg name="physical_params" default="$(find zerog_description)/config/ur10e/physical_parameters.yaml"/>
    <arg name="visual_params" default="$(find zerog_description)/config/ur10e/visual_parameters.yaml"/>
   -->

    <!--common parameters -->
